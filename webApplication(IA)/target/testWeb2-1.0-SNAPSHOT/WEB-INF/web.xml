<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
   
    <servlet>
        <servlet-name>mysql_employeeAdd</servlet-name>
        <servlet-class>com.Servlets.addEmployee</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mysql_employeeAdd</servlet-name>
        <url-pattern>/addEmployee</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>mysql_companyAdd</servlet-name>
        <servlet-class>com.Servlets.addCompany</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mysql_companyAdd</servlet-name>
        <url-pattern>/addCompany</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>mysql_FacilityAdd</servlet-name>
        <servlet-class>com.Servlets.addFacility</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mysql_FacilityAdd</servlet-name>
        <url-pattern>/addFacility</url-pattern>
    </servlet-mapping>
    
</web-app>
